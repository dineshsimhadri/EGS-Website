{
  "name": "egsonweb",
  "private": true,
  "description": "Express Global Solutions Website",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node bin/server",
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "serve": "better-npm-run serve",
    "deploy": "better-npm-run deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "dev": {
      "command": "npm start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "npm run clean && node bin/compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "serve": {
      "command": "npm start",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy": {
      "command": "npm run compile && npm start",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/express-global-solutions/website.git"
  },
  "author": "whoisandie",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.5",
    "bootstrap-loader": "^1.0.5",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "cssnano": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.14",
    "postcss-loader": "^0.8.0",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
