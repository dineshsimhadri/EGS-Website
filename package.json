    "hpp": "^0.2.0",
    "morgan": "^1.7.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-ga": "^1.2.0",
    "react-helmet": "^2.3.1",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-slick": "^0.11.1",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
